{
  "compilerOptions": {
      "outDir": "./dist/",
      "allowJs": true,
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "typeRoots": ["types", "node_modules/@types"]
  },
  "include": [
    "src"
  ]
}